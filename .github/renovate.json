{
  "extends": [
    "config:base"
  ],
  "rollbackPrs": true,
  "stabilityDays": 3,
  "labels": ["renovate"],
  "pre-commit": {
    "enabled": true,
    "fileMatch": ["^modules/github-repo/templates/pre-commit-config.yaml\\.tftpl"]
  },
  "github-actions": {
    "fileMatch": [
      "^(workflows|actions|{{cookiecutter.name}}?|\.github\/workflows|modules\/github-repo\/template\/workflows)\/[^/]+\.ya?ml$",
      "(^|\/)action\.ya?ml$"
    ]
  },
  "kubernetes": {
    "fileMatch": ["k8s/.+\\.yaml$"]
  },
  "hostRules": [
    {
      "matchHost": "https://api.github.com/repos/superbestfriends/",
      "encrypted": {
        "token": ""
      }
    },
    {
      "matchHost": "https://npm.pkg.github.com/",
      "hostType": "npm",
      "encrypted": {
        "token": ""
      }
    },
    {
      "matchHost": "https://ghcr.io/superbestfriends/",
      "encrypted": {
        "token": ""
      }
    },
    {
      "matchHost": "https://maven.pkg.github.com/superbestfriends/",
      "hostType": "maven",
      "encrypted": {
        "token": ""
      }
    }
  ],
  "customEnvVariables": {
    "GITHUB_COM_TOKEN": "github.com/some-org,github.com/some-other-org,gitlab.com/some-org"
  }
}
