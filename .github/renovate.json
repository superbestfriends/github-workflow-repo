{
  "extends": [
    "config:base"
  ],
  "rollbackPrs": true,
  "stabilityDays": 3,
  "labels": ["renovate"],
  "pre-commit": {
    "enabled": true,
    "fileMatch": ["^modules/github-repo/templates/pre-commit-config.yaml\\.tftpl"]
  },
  "github-actions": {
    "fileMatch": [
      "^(workflows/**|actions|{{cookiecutter.name}}\/.github/workflows?|\.github\/workflows|modules\/github-repo\/template\/workflows)\/[^/]+\.ya?ml$",
      "(^|\/)action\.ya?ml$"
    ]
  },
  "hostRules": [
    {
      "matchHost": "https://maven.pkg.github.com/superbestfriends/",
      "encrypted": {
        "token": ""
      }
    },
    {
      "matchHost": "https://ghcr.io/superbestfriends",
      "encrypted": {
        "token": ""
      }
    }
  ]
}
